{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": false,
        "quotemark": false,
        "ordered-imports": ["false"],
        "interface-name": ["never-prefix"],
        "member-access": false,
        "no-submodule-imports": false,
        "trailing-comma": false,
        "object-literal-sort-keys": false,
        "indent": false,
        "max-classes-per-file": [false],
        "no-namespace": [false],
        "no-shadowed-variable": false,
        "no-console": false
    },
    "rulesDirectory": []
}